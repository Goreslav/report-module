{
  "name": "@goreslav/report-module",
  "version": "1.0.0",
  "description": "A powerful Nuxt 3 module for creating ticket reporting modals with ease",
  "repository": {
    "type": "git",
    "url": "https://github.com/Goreslav/report-module.git"
  },
  "homepage": "https://github.com/Goreslav/report-module",
  "bugs": {
    "url": "https://github.com/Goreslav/report-module/issues"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "modal",
    "report",
    "ticket",
    "bug-report",
    "feedback",
    "vue"
  ],
  "author": "Goreslav",
  "scripts": {
    "build": "tsc && cp -r src/runtime dist/ && node -e \"const fs=require('fs'); const content=fs.readFileSync('dist/module.js','utf8').replace(/export default/,'module.exports =').replace(/import\\s+{([^}]+)}\\s+from\\s+['\\\"']([^'\\\"']+)['\\\"']/g,'const {$1} = require(\\\"$2\\\")'); fs.writeFileSync('dist/module.cjs', content);\"",
    "build:unbuild": "unbuild",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "unbuild --stub && nuxi prepare playground",
    "release": "npm run build && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "npm run dev:prepare",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.0",
    "unbuild": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.0",
    "@nuxt/schema": "^3.8.0",
    "@nuxt/test-utils": "^3.8.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.54.0",
    "nuxt": "^3.8.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.8.3",
    "vitest": "^0.34.6",
    "vue": "latest"
  },
  "peerDependencies": {
    "tailwindcss": "^3.0.0"
  }
}
