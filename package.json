{
  "name": "@goreslav/report-module",
  "version": "1.1.6",
  "description": "A Nuxt 3 module for creating ticket reporting modals",
  "repository": {
    "type": "git",
    "url": "https://github.com/Goreslav/report-module.git"
  },
  "homepage": "https://github.com/Goreslav/report-module",
  "bugs": {
    "url": "https://github.com/Goreslav/report-module/issues"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "modal",
    "report",
    "ticket",
    "bug-report",
    "feedback",
    "vue"
  ],
  "author": "goreslav",
  "scripts": {
    "build": "pnpm run clean && pnpm run build:tsc && pnpm run build:copy && pnpm run build:cjs",
    "clean": "rm -rf dist",
    "build:tsc": "tsc",
    "build:copy": "cp -r src/runtime dist/ && cp dist/module.js dist/module.mjs",
    "build:cjs": "node scripts/create-cjs.js",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "unbuild --stub && nuxi prepare playground",
    "release": "pnpm run build && changelogen --release && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.0",
    "unbuild": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.0",
    "@nuxt/schema": "^3.8.0",
    "@nuxt/test-utils": "^3.8.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.54.0",
    "nuxt": "^3.8.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.8.3",
    "vitest": "^0.34.6",
    "vue": "latest"
  },
  "peerDependencies": {
    "tailwindcss": "^3.0.0"
  }
}
