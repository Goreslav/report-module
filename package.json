{
  "name": "report-module",
  "version": "1.0.0",
  "description": "Nuxt module for ticket reporting system",
  "repository": "your-username/report-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "unbuild --stub && nuxi prepare playground",
    "release": "npm run build && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "npm run dev:prepare"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.0",
    "@nuxt/schema": "^3.8.0",
    "@nuxt/test-utils": "^3.8.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.54.0",
    "nuxt": "^3.8.0",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6",
    "vue": "latest",
    "tailwindcss": "^3.3.0"
  },
  "peerDependencies": {
    "tailwindcss": "^3.0.0"
  }
}
